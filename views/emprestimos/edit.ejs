<main role="main" class="col-10 ml-auto px-4">
    <div class="d-flex justify-content-between align-items-center mt-4 border-bottom pb-2 mb-3">
      <h1 class="h2">Editar</h1>
      <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
        <div class="btn-group mr-2" role="group" aria-label="First group">
          <a class="btn btn-primary" href="/chaves"><i class="fa fa-arrow-left" aria-hidden="true"></i> Voltar</a>
        </div>
      </div>
  </div>
  <!--section da main-->
  <section class="row">
   
  <div class="col-12"> 
    <div class="card">
      <div class="card-header">Detalhar emprestimo</div>
      <br>
      <form class="col-6" method="POST" action="/emprestimos/salvar/<%= emprestimo.id %> ">
      <div class="form-group">
          <label for="exampleInputEmail1">Endereço imóvel:</label>
     
          <input type="text" disabled="true" name="endereco" class="form-control" value="<%= emprestimo.chave.endereco %>" id="exampleInputText1" aria-describedby="emailHelp">
      </div>
        <div class="row">
            
          <div class="form-group col-12">
            <label for="exampleInputPassword1">Observações:</label>
            <textarea name="obs" class="form-control" name="" id="obs" cols="30" rows="5"></textarea>
          </div>
          
          
        <!-- foms chaves-->
        <div class="form-group col-3">
            <label for="exampleInputPassword1">Tipo Saida</label>
            <select name="tipo" class="form-control" id="">
               
                <option> <%= emprestimo.tipo %> </option>
                <option value="Serviço">Serviço</option>
                <option value="Locatario">Locatário</option>
                <option value="Proprietario">Proprietário</option>
                 
            </select>
          </div>
        <div class="form-group col-3">
          <label for="exampleInputPassword1">Chaves comuns:</label>
          <input type="text" name="chavescomuns" class="form-control" disabled="true" value="<%= emprestimo.chave.qtdchavescomuns %>" id="exampleInputText1" aria-describedby="emailHelp">
        </div>
        <div class="form-group col-3">
          <label for="exampleInputPassword1">Chaves tetras:</label>
          <input type="text" name="chavestetra" class="form-control" disabled="true" value="<%= emprestimo.chave.qtdchavestetras %>" id="exampleInputText1" aria-describedby="emailHelp">
        </div>
        <div class="form-group col-3">
          <label for="exampleInputPassword1">Controles:</label>
          <input type="text" name="controles" class="form-control" disabled="true" value="<%= emprestimo.chave.qtdcontroles %>"id="exampleInputText1" aria-describedby="emailHelp">
      <button type="submit" class="btn btn-primary">Salvar</button>
      </form>
      <br>
    </div>
  </div>
